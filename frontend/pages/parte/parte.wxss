/* pages/parte/parte.wxss */
@import "../page.wxss";

.main{
  width: 800rpx;
}

.wrapper{
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 100%;
  height: 100%
}

.progress{
  height: 100rpx;
  background-color:white;
  text-align: center;
}

.head{
  width: 100%;
  height: 5%;
  color: rgba(255, 255, 255, 0);
}

.title{
  display: flex;
  flex-direction: column;
  height: 8%;
  background-color: rgba(255, 255, 255, 0);
  text-align: center;
  color: rgb(255, 255, 255);
  font-weight: 500;
  font-size: large;
  align-content: center;
  justify-content: center;
}

.table{
  background-color:white;
  align-items: center;
  text-align: center;
}

.signature{
  border: 5rpx solid #ccc;
  width:80%;
  height:200px;
  margin: 5% 10%;
}

.table_tr{
  background-color: white;
  display: flex;
  flex-direction: row;
  height: 100rpx;
}

.buttons{
  text-align: center;
  flex-direction: column;
  height: 85%;
  background-color:rgb(236, 236, 236);
  color: rgb(142,142,142);
  width: 81%;
  margin-left: 10%;
  margin-right: 10%;
}

.button_area{
  height: 400rpx;
  background-color:white;
  text-align: center;
  display: flex;
  flex-direction: row;
}

.md_button{
  background-color: rgb(99, 165, 242);
  width: 30%;
  height: 20%;
  color: white;
  top: 30rpx;
  font-size: small;
}

