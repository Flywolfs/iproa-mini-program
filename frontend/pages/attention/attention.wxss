/* attention */
.att_wrapper{
  height: 1400rpx;
}

.titleItem{
  flex-grow: 1;
  color: white;
  font-weight: 500;
  font-size: x-large;
}

.attention{
  display: flex;
  flex-direction: row;
  height: 80rpx;
  background-color: white;
  color: rgb(187, 99, 99);
  text-align: center;
  font-size: small;
}

.at_cell{
  width: 50%;
  margin: 2%,5%,0;
}
   
.att_selections{
  width: 85%;
  height: 6rem;
  align-self: center;
}

.att_selection_button{
  height: 50%;
  width: 100%;
  display: flex;
  align-items: center;
}

/* viptype */
.viptype_wrapper{
  height: 1400rpx;
}

.viptype_title{
  text-align: center;
  color: rgb(255, 255, 255);
  font-weight: 500;
  font-size:x-large;
  align-content: center;
  justify-content: center;
} 

.viptype_sm_table{
  /* justify-content: center; */
