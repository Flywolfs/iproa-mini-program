}

.viptype_selections{
  width: 90%;
  height: 90%;
}

.viptype_selection{
  height: 25%;
  width: 100%;
}

.selection_button{
  display: flex;
  width: 10%;
  height: 100%;
  margin-left: 2%;
  margin-right: 2%;
  justify-content: center;
  align-items: center;
}

.type_descs{
  width: 85%;
  height: 100%;
  border: 1px solid  rgb(96, 165, 242);
  display: flex;
  flex-direction: column;
}

.type_desc{
  height: 50%;
